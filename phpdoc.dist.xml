<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>memcached / yrmcds client for PHP</title>
    <parser>
        <default-package-name>yrmcds</default-package-name>
        <target>output</target>
    </parser>
    <transformer>
        <target>html</target>
    </transformer>
    <logging>
        <level>warn</level>
        <paths>
            <default>output/{DATE}.log</default>
            <errors>output/{DATE}.errors.log</errors>
        </paths>
    </logging>
    <transformations>
        <template name="clean" />
    </transformations>
    <files>
        <file>yrmcds-api.php</file>
        <file>yrmcds_session.php</file>
    </files>
</phpdoc>
